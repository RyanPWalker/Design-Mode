{
  "name": "Design Mode",
  "version": "1.1.0",
  "description": "Edit a webpage's text",
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Enable Design Mode"
  },
  "icons": {
    "16": "images/cursor16.png",
    "32": "images/cursor32.png",
    "64": "images/cursor64.png",
    "128": "images/cursor128.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  }
}